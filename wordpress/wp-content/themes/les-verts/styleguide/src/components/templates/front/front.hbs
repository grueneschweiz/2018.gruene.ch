<div class="page page--home">
    {{render '@o-header'}}
    <main class="page__main">
        <section class="page__campaign">
            {{render '@o-campaign'}}
        </section>
        <div class="page__grid-wrapper">
            <section class="page__grid">
                <div class="page__article">
                    {{render '@o-vote--3-proposals'}}
                </div>
                <div class="page__article">
                    {{render '@o-front-article--single'}}
                </div>
                <div class="page__article">
                    {{render '@o-front-article--oton' }}
                </div>
                <div class="page__article">
                    {{render '@o-front-article' }}
                </div>
                <div class="page__article page__article--2-col">
                    {{render '@o-positions' }}
                </div>
                <div class="page__article page__article--2-col">
                    {{render '@o-front-events' }}
                </div>
            </section>
            <section class="page__article page__article--full-width">
                {{render '@o-people-slider'}}
            </section>
            <section class="page__engagement-map">
                {{render '@o-engagement-map'}}
            </section>
        </div>
    </main>
    {{render '@o-footer'}}
</div>
